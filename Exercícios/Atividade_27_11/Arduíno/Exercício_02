int cameraData[10];  // Declaração do vetor com 10 elementos

void setup() {
  Serial.begin(9600);  // Inicializa a comunicação serial

  // Lê o valor do pino A1 e armazena no índice 3 do vetor cameraData
  cameraData[3] = analogRead(A1);  

  // Exibe o valor armazenado no índice 3 do vetor
  Serial.print("Valor lido de A1 (armazenado em cameraData[3]): ");
  Serial.println(cameraData[3]);
}

void loop() {
  // O loop principal pode estar vazio para este exemplo
}
