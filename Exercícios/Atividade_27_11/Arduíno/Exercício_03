// Definir um vetor de floats chamado "temperaturas" com 5 valores
float temperaturas[] = {25.5, 26.0, 24.8, 27.3, 26.5};

void setup() {
  // Inicializa a comunicação serial para monitorar a saída
  Serial.begin(9600);

  // Imprimir os valores do vetor "temperaturas"
  for (int i = 0; i < 5; i++) {
    Serial.print("temperaturas[");
    Serial.print(i);
    Serial.print("] = ");
    Serial.println(temperaturas[i]);
  }
}

void loop() {
  // O loop pode ficar vazio, pois não há necessidade de atualizar o vetor
}
