int cameraData[10];
//Exemplo de uso

int cameraData[10];  // Declara o vetor

void setup() {
  Serial.begin(9600);
  
  // Atribuindo valores ao vetor
  for (int i = 0; i < 10; i++) {
    cameraData[i] = i * 10;  // Preenchendo o vetor com mÃºltiplos de 10
  }
  
  // Imprimindo os valores armazenados no vetor
  for (int i = 0; i < 10; i++) {
    Serial.println(cameraData[i]);  // Exibe cada valor no Serial Monitor
  }
}

void loop() {
  // O loop principal pode estar vazio para este exemplo
}
